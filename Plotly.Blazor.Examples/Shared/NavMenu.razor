<MudPaper Elevation="1" Style="position: sticky; top: 0px; z-index: 100">
    <MudToolBar>
        <MudIconButton Icon="@Icons.Material.Filled.Menu" OnClick="OpenOrCloseDrawer" Edge="Edge.Start" />
    </MudToolBar>
</MudPaper>

<MudDrawer @bind-Open="@open" ClipMode="DrawerClipMode.Never" Breakpoint="Breakpoint.Xl" Elevation="0" Variant="@DrawerVariant.Responsive">
    <MudDrawerHeader>
        <MudText Typo="Typo.h6">Examples</MudText>
    </MudDrawerHeader>
    <MudNavMenu>
        <MudNavLink Match="NavLinkMatch.All" Href="/">Scatter</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/live-data">Live Data</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/bar">Bar</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/pie">Pie</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/box">Box</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/candlestick">Candlestick</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/multipleaxes">Multiple Axes</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/shapes">Shapes</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/map">Map</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/surface">Surface</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/ribbon">Ribbon</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/heatmap">HeatMap</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/scattergl">ScatterGl</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/indicator">Indicator</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/smith">Smith</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/hover">HoverEvent</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="/relayout">RelayoutEvent</MudNavLink>
    </MudNavMenu>
</MudDrawer>